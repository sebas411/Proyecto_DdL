let delim = ["\s\t\n"]
let ws = delim+
let digit = ['0'-'9']
let digits = digit+
let number = digits('.'digits)?('E'['+''-']?digits)?

rule tokens = 
    ws        { cout << "ws"; }  (* Prueba de archivo sin ejecucion inicial ni final *)
  | number    { cout << "NUMBER"; }
  | '+'       { cout << "+"; }
  | '-'       { cout << "-"; }
  | '*'       { cout << "*"; }
  | '/'       { cout << "/"; }

