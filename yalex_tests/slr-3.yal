let delim = [' ''\t''\n']
let ws = delim+

rule tokens = 
    ws        
  | 'a'           { cout << "token of a" << endl; }
  | "abb"         { cout << "token of abb" << endl; }
  | 'a'*'b'+      { cout << "token of a*b+" << endl; }

