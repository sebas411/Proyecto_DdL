let delim = [' ''\t''\n']
let ws = delim+
let letter = ['a'-'z''A'-'Z']
let digit = ['0'-'9']
let conditional = "if"
let id = letter(letter|digit)*

rule tokens = 
    ws          { cout << "ws" << endl; }
  | conditional { cout << "conditional" << endl; }               (* Comentario en return *)
  | id          { cout << "identifier" << endl; }
  | '+'         { cout << "addition" << endl; }
  | '*'         { cout << "multiplication" << endl; }

